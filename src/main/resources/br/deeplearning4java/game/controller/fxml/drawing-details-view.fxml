<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" prefHeight="400.0" prefWidth="300.0" spacing="10.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="br.deeplearning4java.game.controller.DrawingDetailsController">

    <ImageView fx:id="drawingImageView" fitHeight="200.0" fitWidth="200.0" />

    <Label fx:id="drawID" text="ID: " />

    <Label fx:id="predictionCategoryLabel" text="Categorie: " />

    <Label fx:id="predictionProbabilityLabel" text="Probability: " />

    <Label fx:id="roundIdLabel" text="Round ID: " />

    <Button fx:id="deleteButton" text="Delete" textFill="WHITE" style="-fx-background-color: #cd0909;"/>
</VBox>
