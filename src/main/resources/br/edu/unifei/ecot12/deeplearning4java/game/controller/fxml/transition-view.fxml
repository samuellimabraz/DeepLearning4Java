<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="AnchorPane" prefHeight="620.0" prefWidth="770.0" stylesheets="@../css/transition-style.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="br.edu.unifei.ecot12.deeplearning4java.game.controller.TransitionController">
    <Label fx:id="roundLabel" layoutX="339.0" layoutY="14.0" text="Drawing {numRound} / 6">
      <font>
         <Font size="17.0" />
      </font></Label>
    <Label fx:id="category" alignment="CENTER" contentDisplay="CENTER" layoutX="297.0" layoutY="251.0" lineSpacing="1.0" prefHeight="28.0" prefWidth="180.0" text="category " textAlignment="CENTER" wrapText="true">
      <font>
         <Font size="22.0" />
      </font></Label>
    <Label fx:id="drawLabel" layoutX="362.0" layoutY="209.0" text="Draw">
      <font>
         <Font size="22.0" />
      </font></Label>
    <Label fx:id="instructionLabel" alignment="CENTER" layoutX="285.0" layoutY="293.0" prefHeight="35.0" prefWidth="204.0" text="in under 20 seconds">
      <font>
         <Font size="19.0" />
      </font></Label>
    <Button fx:id="confirmButton" layoutX="316.0" layoutY="352.0" mnemonicParsing="false" onAction="#handleStartButtonAction" opacity="0.74" prefHeight="39.0" prefWidth="142.0" text="Got It!" textAlignment="CENTER" textOverrun="WORD_ELLIPSIS" underline="true" wrapText="true">
        <cursor>
            <Cursor fx:constant="HAND" />
        </cursor>
      <font>
         <Font size="22.0" />
      </font>
    </Button>
    <cursor>
        <Cursor fx:constant="DEFAULT" />
    </cursor>
</AnchorPane>
